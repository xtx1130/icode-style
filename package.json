{
  "name": "tb-format",
  "version": "2.0.15",
  "description": "icode code style standard",
  "bin": {
    "tbformat": "./bin/format"
  },
  "main": "./src/index.js",
  "scripts": {
    "test": "jest",
    "lint": "standard src/*.js --verbose --fix"
  },
  "keywords": [
    "icode",
    "style"
  ],
  "author": "tb-fe",
  "license": "MIT",
  "dependencies": {
    "@xtx1130/tb-log": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "comment-parser": "^0.5.4",
    "eslint": "^5.10.0",
    "fs-extra": "^7.0.1",
    "klaw": "^3.0.0",
    "micromatch": "^3.1.10",
    "minimist": "^1.2.0",
    "prettier": "^1.15.3",
    "through2": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "standard": "^12.0.1"
  },
  "jest": {
    "testMatch": [
      "**/test/*.js"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!**/test/**",
      "!**/benchmark/**",
      "**/*.js",
      "!**/coverage/**"
    ]
  }
}
